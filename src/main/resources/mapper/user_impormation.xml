<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="">
    <select id="selectUserList" resultType="com.board.model.UserDto">
        select * from user_inpormation
    </select>

    <select id="selectUserInfo" resultType="com.board.model.BoardDTO">
        select * from user_inpormation
        where id = #{userId}
    </select>

</mapper>